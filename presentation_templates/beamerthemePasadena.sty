\mode<presentation>

\useinnertheme[shadow=true]{rounded}
\useoutertheme{infolines}

\definecolor{darkorange}{rgb}{1,.4,0}
\definecolor{darkerorange}{rgb}{.8,.2,0}

\setbeamercolor{section in toc}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=darkorange!80!gray}
\setbeamercolor*{palette primary}{fg=darkorange!60!black,bg=gray!30!white}
\setbeamercolor*{palette secondary}{fg=darkorange!70!black,bg=gray!15!white}
\setbeamercolor*{palette tertiary}{bg=darkorange!80!black,fg=gray!10!white}
\setbeamercolor*{palette quaternary}{fg=darkorange,bg=gray!5!white}

\setbeamercolor*{sidebar}{fg=darkorange,bg=gray!15!white}

\setbeamercolor*{palette sidebar primary}{fg=darkorange!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=darkorange!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=gray!10!white}

\setbeamercolor{titlelike}{parent=palette primary,fg=darkerorange}
\setbeamercolor{frametitle}{bg=gray!10!white}
\setbeamercolor{frametitle right}{bg=gray!60!white}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamerfont{block title}{size={}}
\setbeamercolor{titlelike}{parent=structure,bg=white}

\addtobeamertemplate{title page}{}{\hspace{\stretch{1}}\includegraphics[width=.12\textwidth]{Caltech_logo_orange.png}}

\mode
<all>
